{
  "name": "Tab Shortcuts - Pinned & Unpinned",
  "description": "Pinned tabs steal your shortcuts, get them back. This add-on allows you to switch to pinned or unpinned tabs with custom shortcuts (Alt+N by default).",
  "homepage_url": "https://github.com/notSagyo/tab-shortcuts/",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": ["tabs"],
  "browser_specific_settings": {
    "gecko": {
      "id": "tab-shortcuts@sagyo.com",
      "strict_min_version": "58.0"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "commands": {
    "select-unpinned-tab-1": { "suggested_key": { "default": "Alt+1" }, "description": "1st unpinned tab" },
    "select-unpinned-tab-2": { "suggested_key": { "default": "Alt+2" }, "description": "2nd unpinned tab" },
    "select-unpinned-tab-3": { "suggested_key": { "default": "Alt+3" }, "description": "3rd unpinned tab" },
    "select-unpinned-tab-4": { "suggested_key": { "default": "Alt+4" }, "description": "4th unpinned tab" },
    "select-unpinned-tab-5": { "suggested_key": { "default": "Alt+5" }, "description": "5th unpinned tab" },
    "select-unpinned-tab-6": { "suggested_key": { "default": "Alt+6" }, "description": "6th unpinned tab" },
    "select-unpinned-tab-7": { "suggested_key": { "default": "Alt+7" }, "description": "7th unpinned tab" },
    "select-unpinned-tab-8": { "suggested_key": { "default": "Alt+8" }, "description": "8th unpinned tab" },
    "select-unpinned-tab-9": { "description": "9th unpinned tab" },
    "select-unpinned-tab-10": { "suggested_key": { "default": "Alt+9" }, "description": "last unpinned tab" },
    
    "select-pinned-tab-1": { "description": "1st pinned tab" },
    "select-pinned-tab-2": { "description": "2nd pinned tab" },
    "select-pinned-tab-3": { "description": "3rd pinned tab" },
    "select-pinned-tab-4": { "description": "4th pinned tab" },
    "select-pinned-tab-5": { "description": "5th pinned tab" },
    "select-pinned-tab-6": { "description": "6th pinned tab" },
    "select-pinned-tab-7": { "description": "7th pinned tab" },
    "select-pinned-tab-8": { "description": "8th pinned tab" },
    "select-pinned-tab-9": { "description": "9th pinned tab" },
    "select-pinned-tab-10": { "description": "last pinned tab" }
  } 
}
