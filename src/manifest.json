{
  "name": "Tab Shortcuts - Pinned & Unpinned",
  "description": "Pinned tabs steal your shortcuts, get them back. This add-on allows you to switch to pinned or unpinned tabs with custom shortcuts (Alt+N by default). Also has support for Zen Browser's essential tabs.",
  "homepage_url": "https://github.com/notSagyo/tab-shortcuts/",
  "manifest_version": 2,
  "version": "1.1",
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_popup": "./popup/popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{0a828b09-11e5-44af-8a3d-e0d4ee2b5e12}",
      "strict_min_version": "58.0"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "commands": {
    "select-unpinned-tab-1": { "suggested_key": { "default": "Alt+1" }, "description": "1st unpinned tab" },
    "select-unpinned-tab-2": { "suggested_key": { "default": "Alt+2" }, "description": "2nd unpinned tab" },
    "select-unpinned-tab-3": { "suggested_key": { "default": "Alt+3" }, "description": "3rd unpinned tab" },
    "select-unpinned-tab-4": { "suggested_key": { "default": "Alt+4" }, "description": "4th unpinned tab" },
    "select-unpinned-tab-5": { "suggested_key": { "default": "Alt+5" }, "description": "5th unpinned tab" },
    "select-unpinned-tab-6": { "suggested_key": { "default": "Alt+6" }, "description": "6th unpinned tab" },
    "select-unpinned-tab-7": { "suggested_key": { "default": "Alt+7" }, "description": "7th unpinned tab" },
    "select-unpinned-tab-8": { "suggested_key": { "default": "Alt+8" }, "description": "8th unpinned tab" },
    "select-unpinned-tab-9": { "description": "9th unpinned tab" },
    "select-unpinned-tab-100": { "suggested_key": { "default": "Alt+9" }, "description": "Last unpinned tab" },

    "select-pinned-tab-1": { "description": "1st pinned tab" },
    "select-pinned-tab-2": { "description": "2nd pinned tab" },
    "select-pinned-tab-3": { "description": "3rd pinned tab" },
    "select-pinned-tab-4": { "description": "4th pinned tab" },
    "select-pinned-tab-5": { "description": "5th pinned tab" },
    "select-pinned-tab-6": { "description": "6th pinned tab" },
    "select-pinned-tab-7": { "description": "7th pinned tab" },
    "select-pinned-tab-8": { "description": "8th pinned tab" },
    "select-pinned-tab-9": { "description": "9th pinned tab" },
    "select-pinned-tab-100": { "description": "Last pinned tab" },

    "select-essential-tab-1": { "description": "1st essential tab (Zen Browser only)" },
    "select-essential-tab-2": { "description": "2nd essential tab (Zen Browser only)" },
    "select-essential-tab-3": { "description": "3rd essential tab (Zen Browser only)" },
    "select-essential-tab-4": { "description": "4th essential tab (Zen Browser only)" },
    "select-essential-tab-5": { "description": "5th essential tab (Zen Browser only)" },
    "select-essential-tab-6": { "description": "6th essential tab (Zen Browser only)" },
    "select-essential-tab-7": { "description": "7th essential tab (Zen Browser only)" },
    "select-essential-tab-8": { "description": "8th essential tab (Zen Browser only)" },
    "select-essential-tab-9": { "description": "9th essential tab (Zen Browser only)" },
    "select-essential-tab-10": { "description": "10th essential tab (Zen Browser only)" },
    "select-essential-tab-11": { "description": "11th essential tab (Zen Browser only)" },
    "select-essential-tab-100": { "description": "Last essential tab (Zen Browser only)" }
  }
}
